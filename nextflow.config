process {
  executor = 'slurm'
  queue = 'compute'        // or whatever your cluster partition is
  cpus = 4                 // default number of CPUs per job
  memory = '16 GB'          // default memory
  time = '1h'              // default runtime limit
  clusterOptions = '--account=scw1557'
}

timeline {
  enabled = true
  file = 'timeline.html'
  overwrite = true
}

trace {
  enabled = true
  file = 'trace.txt'
  overwrite = true
}